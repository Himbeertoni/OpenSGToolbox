<?xml version="1.0"?>

<FieldContainer
	name="SplitPanel"
	parent="Container"
	library="UserInterface"
	pointerfieldtypes="both"
	structure="concrete"
	systemcomponent="false"
	parentsystemcomponent="false"
	decoratable="false"
	useLocalIncludes="true"
>
A UI Split Panel.
	<Field
		name="MinComponent"
		type="ComponentPtr"
		cardinality="single"
		visibility="external"
		header="Component/OSGComponent.h"
		defaultValue="NullFC"
		access="public"
	>
	</Field>
	<Field
		name="MaxComponent"
		type="ComponentPtr"
		cardinality="single"
		visibility="external"
		header="Component/OSGComponent.h"
		defaultValue="NullFC"
		access="public"
	>
	</Field>
	<Field
		name="DividerSize"
		type="UInt32"
		cardinality="single"
		visibility="external"
		defaultValue="3"
		access="public"
	>
	</Field>
	<Field
		name="DividerPosition"
		type="Real32"
