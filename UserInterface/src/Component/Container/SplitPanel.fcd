	>
	</Field>
	<Field
		name="Alignment"
		type="UInt32"
		cardinality="single"
		visibility="external"
		access="public"
		defaultHeader="Util/OSGUIDefines.h"
		defaultValue="HORIZONTAL_ALIGNMENT"
